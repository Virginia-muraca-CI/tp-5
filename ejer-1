#include <stdio.h>
//Calcular la resistencia total de 5 resistencias en serie cargadas en un vector,
//mostrar cada una de ellas y la resistencia total.
int main()
{   int i, resistencia[5],rt=0;

    for(i=0;i<5;i++)
    {
        printf("ingrese el valor de la resistencia:\n");
        scanf("%d",&resistencia[i]);
        printf("resistencia N%d:%d\n",i+1,resistencia[i]);
        rt=rt+resistencia[i];
    }
    printf("el valor de la resistencia total: %d\n",rt);
    
    return 0;
}
